
@typeparam TValue

<div class="form-group row">
    <label for="@Id" class="col-sm-2 col-form-label">@Label</label>
    <div class="col-sm-10">
        <input type="text" readonly class="form-control-plaintext" id="@Id" value="@Value">
    </div>
</div>

@functions
{
    [Parameter]
    string Id { get; set; }

    [Parameter]
    string Label { get; set; }

    [Parameter]
    TValue Value { get; set; }
}
